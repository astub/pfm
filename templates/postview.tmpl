	<div id="" class="box rounded pure-g">
	
		<div class="pure-u-1 pure-u-md-1-2">
			<h4 class="subTitle">{{.Title}}</h4> 
			<p>{{.Date}}</p> 
			<br /> 
			<h4 class="subTitle">Detail</h4> 
			<p>{{.Details}}</p> 
